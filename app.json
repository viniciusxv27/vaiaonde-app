{
  "expo": {
    "name": "Vai Aonde",
    "slug": "vaiaonde",
    "scheme": "vaiaonde",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/default-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#070707"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/default-icon.png",
        "backgroundColor": "#070707"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
